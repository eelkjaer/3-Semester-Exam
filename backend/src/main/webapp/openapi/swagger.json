  /TheQueue_Backend/api/courses/count:
    get:
      summary: Count courses in db
      description: Count courses in db
      operationId: countCoursesInDb
      responses:
        "200":
          description: Count courses in db
          headers:
            Connection:
              schema:
                type: string
                example: keep-alive
            Content-Length:
              schema:
                type: string
                example: "12"
            Date:
              schema:
                type: string
                example: "Tue, 11 May 2021 17:59:35 GMT"
            Keep-Alive:
              schema:
                type: string
                example: timeout=20
