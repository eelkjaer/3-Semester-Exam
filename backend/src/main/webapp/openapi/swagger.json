info:
  title: The Queue
  version: 1.0.0
  description: "API Documentation used for the backend of our 3. Semester project"
  contact: {
    name: Emil Elkjær Nielsen,
    email: cph-en93@cphbusiness.dk
  }
  license: {
    name: MIT,
    url: "https://github.com/codergram/TheQueue_Backend/blob/main/LICENSE"
  }
servers:
  - url: "https://api.codergram.me"
    description: Production
  - url: "http://127.0.0.1:8080"
    description: Localhost
paths:
  /TheQueue_Backend/api/:
    get:
      summary: API heartbeat
      description: API heartbeat
      operationId: apiHeartbeat
      responses:
        "200":
          description: API heartbeat
          headers:
            Connection:
              schema:
                type: string
                example: keep-alive
            Content-Length:
              schema:
                type: string
                example: "19"
            Date:
              schema:
                type: string
                example: "Tue, 11 May 2021 17:59:07 GMT"
            Keep-Alive:
              schema:
                type: string
                example: timeout=20
          content:
            application/json:
              schema:
                type: object
                properties:
                  msg:
                    type: string
                    example: "I'm alive"
              examples:
                API heartbeat:
                  value:
                    msg: "I'm alive"
  /TheQueue_Backend/api/auth/:
    post:
      summary: Get JWT token
      description: Get JWT token
      operationId: getJwtToken
      requestBody:
