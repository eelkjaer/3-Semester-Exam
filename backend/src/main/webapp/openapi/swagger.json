          description: Get JWT token
          headers:
            Connection:
              schema:
                type: string
                example: keep-alive
            Content-Length:
              schema:
                type: string
                example: "314"
            Date:
              schema:
                type: string
                example: "Tue, 11 May 2021 17:50:40 GMT"
            Keep-Alive:
